package forLoops;
import java.util.Scanner;		//header files
public class Sum_till_N {

	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int i=1,sum=0;
		while(i<=n)		//iterate till n
		{
			sum=sum+i;
			i++;
		}
		System.out.println(sum);
	}
}
